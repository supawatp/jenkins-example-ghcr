pipeline {
  agent any
    environment {
    GITHUB_TOKEN=credentials('github-token')
  }
  stages {
    stage('cleanup') {
      steps {
        sh 'docker system prune -a --volumes --force'
      }
    }
    stage('build image') {
      steps {
        sh 'docker build -t supawatp/jenkins-example-ghcr:8.4-213 .'
      }
    }
  }
}
